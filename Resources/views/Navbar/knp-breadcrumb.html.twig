<ol class="breadcrumb">
	{% for item in breadcrumbs %}
		<li class="{{ loop.last ? 'active' : '' }}">
			{% if loop.first %}
				<i class="fa fa-home"></i>
			{% elseif loop.last %}
				{{ item.label|trans }}
			{% else %}
				<a href="{{ item.uri }}">{{ item.label|trans }}</a>
			{% endif %}
		</li>
	{% endfor %}
</ol>
